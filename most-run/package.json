    "sinon": "^6.1.4",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "~2.8.4",
    "xstream": "11.x"
  },
  "scripts": {
    "build": "pnpm run build-cjs && pnpm run build-es6",
