  "main": "lib/cjs/index.js",
  "module": "lib/es6/index.js",
  "typings": "lib/cjs/index.d.ts",
  "types": "lib/cjs/index.d.ts",
  "dependencies": {
    "@cycle/dom": ">=20.4.0",
    "@cycle/run": "*",
